{
	"IPv6": {
		"HIGH,MEDIUM": "Internet Protocol Version 6, offset=%{offset}d, length=%{length}d",
		"LOW": "%{src:F}s -> %{dst:F}s > %{protocol:R1}s"
	},
	"HIGH": {
		"version;Version": "%{}d",
		"hdrLen;Header Length": "%{}d [%{}d*4 = %{hdrLenBytes}d bytes]",
		"totalLength;Total Length": "%{}d bytes",
		"identification;Identification": "0x%{}04X [%{}d]",
		"flags;Flags": "%{}x %{flagsEnum}s",
		"fragOffset;Fragment Offset": "%{}d [%{}d*8 = %{fragOffsetBytes}d]",
		"ttl;Time to Live": "%{}d",
		"protocol;Protocol": "%{:IP_TYPE}s (%{}d)",
		"src;Source Address": "%{:F}s",
		"dst;Destination Address": "%{:F}s"
	},
	"LOW": {
		"totalLength;Total Length": "%{}d bytes",
		"protocol;Protocol": "%{}d [%{:IP_TYPE}s]",
		"src;Source Address": "%{:F}s",
		"dst;Destination Address": "%{:F}s"
	}
}